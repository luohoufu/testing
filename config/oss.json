{
    "local_port": ${{ secrets.LOCAL_PORT }},
    "local_address": "127.0.0.1",
    "servers": [
        {
            "server": "${{ secrets.CONNECT_SERVER }}",
            "server_port": ${{ secrets.CONNECT_PORT }},
            "password": "${{ secrets.CONNECT_KEY }}",
            "timeout": ${{ vars.CONNECT_TIMEOUT }},
            "mode": "${{ vars.CONNECT_MODE }}",
            "method": "${{ secrets.CONNECT_METHOD }}"
        }
    ]
}